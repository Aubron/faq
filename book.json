      "sharing": {
            "facebook": false,
            "twitter": false,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
            "facebook", "google", "twitter",
            "weibo", "instapaper"
        ]
      }
